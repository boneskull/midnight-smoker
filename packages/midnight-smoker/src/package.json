{
  "name": "midnight-smoker",
  "engines": {
    "node": "^16.20.0 || ^18.0.0 || ^20.0.0",
    "npm": ">=7.0.0"
  },
  "imports": {
    "#cli": "./cli/index.ts",
    "#cli/*": "./cli/*.ts",
    "#component": "./component/component/index.ts",
    "#config-file": "./config-file.ts",
    "#constants": "./constants.ts",
    "#machine": "./machine/index.ts",
    "#machine/*": "./machine/*/index.ts",
    "#error": "./error/index.ts",
    "#error/*": "./error/*.ts",
    "#event": "./event/index.ts",
    "#event/*": "./event/*.ts",
    "#executor": "./component/executor/index.ts",
    "#options": "./options/index.ts",
    "#options/*": "./options/*.ts",
    "#pkg-manager": "./component/pkg-manager/index.ts",
    "#pkg-manager/*": "./component/pkg-manager/*.ts",
    "#plugin": "./plugin/index.ts",
    "#plugin/*": "./plugin/*.ts",
    "#reporter": "./component/reporter/index.ts",
    "#reporter/*": "./component/reporter/*.ts",
    "#rule": "./component/rule/index.ts",
    "#rule/*": "./component/rule/*.ts",
    "#schema": "./component/schema/index.ts",
    "#schema/*": "./component/schema/*.ts",
    "#smoker": "./smoker.ts",
    "#util": "./util/index.ts",
    "#util/*": "./util/*.ts"
  },
  "exports": {
    ".": {
      "default": "./index.ts"
    },
    "./plugin": {
      "default": "./plugin/index.ts"
