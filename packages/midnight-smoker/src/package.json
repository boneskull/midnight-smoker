      "default": "./component/rule/index.ts"
    },
    "./reporter": {
      "default": "./component/reporter/index.ts"
    },
    "./executor": {
      "default": "./component/executor/index.ts"
    },
    "./pkg-manager": {
      "default": "./component/pkg-manager/index.ts"
    },
    "./rule-runner": {
      "default": "./component/rule-runner/index.ts"
    },
    "./script-runner": {
      "default": "./component/script-runner/index.ts"
    },
    "./package.json": "./package.json",
    "./constants": {
      "default": "./constants.ts"
    },
    "./controller": {
      "default": "./controller/index.ts"
    },
    "./util": {
      "default": "./util/index.ts"
    }
  }
